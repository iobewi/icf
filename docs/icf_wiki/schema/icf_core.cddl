; ICF Core (decoded) â€” excludes security metadata
icf_core = {
  ? "url": tstr,
  ? "language": tstr .regexp "[A-Za-z]{2}",
  ? "title": tstr .size (1..64),
  ? "pedagogical_tag": {
    "cycle": uint .le 255,
    "subject": uint .le 255,
    "subclass": uint .le 255
  },
  ? "retention_days": uint .le 255,
  ? "expires_at": uint,
  ? "badge_type": uint .le 2,
  ? "system_payload": bstr
}
